#include <iostream>
using namespace std;
class RemoveDuplicate {
   public:
        // function to remove duplicate characters
        char * removeDuplicate(char* str) {
             int index = 1;
             for(int i = 1; str[i]!='\0'; i++) {
                  int j ;
                for(j= 0; j < index; j++) {
                    if(str[i] == str[j]) break;
                }
                
                if(j == index) {
                    str[index] = str[i];
                    index++;
                }
            }
         str[index] = '\0';
         return str;
        }
};
int main(){
	RemoveDuplicate obj;
	char * stringArray = (char *) malloc (sizeof (char)*100);

	cout<<"Please provide string:\n";
	cin>>stringArray;
	char *str1 = obj.removeDuplicate(stringArray);
	cout << str1;
	return 0;
}
